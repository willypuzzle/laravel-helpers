{
    "name": "willypuzzle/laravel-helpers",
    "description": "A General Laravel Helpers package.",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/willypuzzle/laravel-helpers/issues",
        "source": "https://github.com/willypuzzle/laravel-helpers"
    },
    "authors": [
        {
            "name": "Domenico Rizzo",
            "email": "domenico.rizzo@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": ">=5.4.0",
        "illuminate/database": ">=5.4.0",
        "league/json-reference": ">=1.0",
        "lodash-php/lodash-php": ">=0.0.6",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Willypuzzle\\Helpers\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Willypuzzle\\Helpers\\GeneralServiceProvider"
            ]
        }
    }
}
